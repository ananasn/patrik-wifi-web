{"ast":null,"code":"import axios from \"axios\";\nimport SimpleKeyboard from \"./SimpleKeyboard\";\nimport \"./App.css\";\nexport default {\n  name: \"App\",\n  components: {\n    SimpleKeyboard\n  },\n  data: () => ({\n    input: \"\",\n    ssid: \"\",\n    options: []\n  }),\n  mounted: function () {\n    this.loadSSID();\n  },\n  methods: {\n    connect: function (ssid, password) {\n      axios.post(\"http://localhost:8888/connect\", {\n        ssid: ssid,\n        password: password\n      }).then(function (response) {\n        console.log(response);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    loadSSID: function (dd) {\n      axios.get(\"http://localhost:8888/ssid/\").then(response => {\n        this.options = response.data;\n        console.log(response.data);\n      }).catch(e => {\n        console.error(e);\n      });\n    },\n    onChange(input) {\n      this.input = input;\n    },\n    onKeyPress(button) {\n      if (button === \"{enter}\") {\n        // this.connect(this.ssid, this.input)\n        console.log(\"NYA\", this.input, this.ssid);\n      }\n    },\n    onInputChange(input) {\n      this.input = input.target.value;\n    }\n  }\n};","map":{"version":3,"names":["axios","SimpleKeyboard","name","components","data","input","ssid","options","mounted","loadSSID","methods","connect","password","post","then","response","console","log","catch","error","dd","get","e","onChange","onKeyPress","button","onInputChange","target","value"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"flex-container\">\n      <select v-model=\"ssid\" class=\"custom-select\">\n        <option value=\"\" disabled selected hidden>Выберите сеть</option>\n        <option \n          v-for=\"option in options\"\n          v-bind:key=\"option.value\"\n          :value=\"option.value\"\n        >\n          {{ option.text }}\n        </option>\n      </select>\n\n      <button @click=\"count++\">⟳</button>\n    </div>\n    <input\n      :value=\"input\"\n      class=\"input\"\n      @input=\"onInputChange\"\n      type=\"password\"\n      placeholder=\"Введите пароль от Wi-Fi сети\"\n    >\n    <SimpleKeyboard \n      @onChange=\"onChange\" \n      @onKeyPress=\"onKeyPress\" \n      :input=\"input\"\n    />\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\"\n\nimport SimpleKeyboard from \"./SimpleKeyboard\"\nimport \"./App.css\"\n\nexport default {\n  name: \"App\",\n  components: {\n    SimpleKeyboard\n  },\n  data: () => ({\n    input: \"\",\n    ssid: \"\",\n    options: []\n  }),\n  mounted: function() {\n    this.loadSSID();\n  },\n  methods: {\n    connect: function(ssid, password) {\n      axios.post(\"http://localhost:8888/connect\", {\n        ssid: ssid,\n        password: password\n      })\n      .then(function (response) {\n        console.log(response)\n      })\n      .catch(function (error) {\n        console.log(error)\n      });\n    },\n    loadSSID: function(dd) {\n      axios.get(\"http://localhost:8888/ssid/\").then(response => {\n        this.options = response.data\n        console.log(response.data)\n      }).catch(e => {\n        console.error(e)\n      })\n    },\n    onChange(input) {\n      this.input = input\n    },\n    onKeyPress(button) {\n      if (button === \"{enter}\"){\n        // this.connect(this.ssid, this.input)\n        console.log(\"NYA\", this.input, this.ssid)\n      }\n    },\n    onInputChange(input) {\n      this.input = input.target.value\n    }\n  }\n};\n</script>\n\n<style>\n#app {\n  font-family: \"Avenir\", Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n</style>\n"],"mappings":"AAgCA,OAAAA,KAAA;AAEA,OAAAC,cAAA;AACA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,IAAA,EAAAA,CAAA;IACAC,KAAA;IACAC,IAAA;IACAC,OAAA;EACA;EACAC,OAAA,WAAAA,CAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACAC,OAAA,WAAAA,CAAAL,IAAA,EAAAM,QAAA;MACAZ,KAAA,CAAAa,IAAA;QACA