{"ast":null,"code":"import axios from 'axios';\nimport SimpleKeyboard from \"./SimpleKeyboard\";\nimport \"./App.css\";\nexport default {\n  name: \"App\",\n  components: {\n    SimpleKeyboard\n  },\n  data: () => ({\n    input: \"\",\n    ssid: \"\",\n    options: []\n  }),\n  mounted: function () {\n    this.options = [{\n      text: \"One\",\n      value: \"A\"\n    }, {\n      text: \"Two\",\n      value: \"B\"\n    }, {\n      text: \"Three\",\n      value: \"C\"\n    }];\n  },\n  methods: {\n    loadData: function () {\n      axios.get('http://localhost:8888/get').then(response => {\n        this.users = response.data;\n      }).catch(e => {\n        console.error(e);\n      });\n    },\n    onChange(input) {\n      this.input = input;\n    },\n    onKeyPress(button) {\n      if (button === \"{enter}\") {\n        console.log(\"NYA\", this.input, this.ssid);\n      }\n      console.log(\"button\", button);\n    },\n    onInputChange(input) {\n      this.input = input.target.value;\n    }\n  }\n};","map":{"version":3,"names":["axios","SimpleKeyboard","name","components","data","input","ssid","options","mounted","text","value","methods","loadData","get","then","response","users","catch","e","console","error","onChange","onKeyPress","button","log","onInputChange","target"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <select v-model=\"ssid\" class=\"custom-select\">\n      <option value=\"\" disabled selected hidden>Выберите сеть</option>\n      <option \n        v-for=\"option in options\"\n        v-bind:key=\"option.value\"\n        :value=\"option.value\"\n      >\n        {{ option.text }}\n      </option>\n    </select>\n    <input\n      :value=\"input\"\n      class=\"input\"\n      @input=\"onInputChange\"\n      type=\"password\"\n      placeholder=\"Введите пароль от Wi-Fi сети\"\n    >\n    <SimpleKeyboard \n      @onChange=\"onChange\" \n      @onKeyPress=\"onKeyPress\" \n      :input=\"input\"\n    />\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nimport SimpleKeyboard from \"./SimpleKeyboard\";\nimport \"./App.css\";\n\nexport default {\n  name: \"App\",\n  components: {\n    SimpleKeyboard\n  },\n  data: () => ({\n    input: \"\",\n    ssid: \"\",\n    options: []\n  }),\n  mounted: function() {\n    this.options = [\n      { text: \"One\", value: \"A\" },\n      { text: \"Two\", value: \"B\" },\n      { text: \"Three\", value: \"C\" }\n    ]\n  },\n  methods: {\n    loadData: function() {\n      axios.get('http://localhost:8888/get').then(response => {\n        this.users = response.data;\n      }).catch(e => {\n        console.error(e)\n      })\n    },\n    onChange(input) {\n      this.input = input;\n    },\n    onKeyPress(button) {\n      if (button === \"{enter}\"){\n        console.log(\"NYA\", this.input, this.ssid)\n      }\n      console.log(\"button\", button);\n    },\n    onInputChange(input) {\n      this.input = input.target.value;\n    }\n  }\n};\n</script>\n\n<style>\n#app {\n  font-family: \"Avenir\", Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n</style>\n"],"mappings":"AA4BA,OAAAA,KAAA;AAEA,OAAAC,cAAA;AACA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,IAAA,EAAAA,CAAA;IACAC,KAAA;IACAC,IAAA;IACAC,OAAA;EACA;EACAC,OAAA,WAAAA,CAAA;IACA,KAAAD,OAAA,IACA;MAAAE,IAAA;MAAAC,KAAA;IAAA,GACA;MAAAD,IAAA;MAAAC,KAAA;IAAA,GACA;MAAAD,IAAA;MAAAC,KAAA;IAAA,EACA;EACA;EACAC,OAAA;IACAC,QAAA,WAAAA,CAAA;MACAZ,KAAA,CAAAa,GAAA,8BAAAC,IAAA,CAAAC,QAAA;QACA,KAAAC,KAAA,GAAAD,QAAA,CAAAX,IAAA;MACA,GAAAa,KAAA,CAAAC,CAAA;QACAC,OAAA,CAAAC,KAAA,CAAAF,CAAA;MACA;IACA;IACAG,SAAAhB,KAAA;MACA,KAAAA,KAAA,GAAAA,KAAA;IACA;IACAiB,WAAAC,MAAA;MACA,IAAAA,MAAA;QACAJ,OAAA,CAAAK,GAAA,aAAAnB,KAAA,OAAAC,IAAA;MACA;MACAa,OAAA,CAAAK,GAAA,WAAAD,MAAA;IACA;IACAE,cAAApB,KAAA;MACA,KAAAA,KAAA,GAAAA,KAAA,CAAAqB,MAAA,CAAAhB,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}